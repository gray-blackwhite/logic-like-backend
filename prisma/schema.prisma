generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Suggestion {
  id          String @id
  name        String
  description String
  votes       IP[]

  @@map("suggestions")
}

model IP {
  value String       @id
  votes Suggestion[]

  @@map("ips")
}
